{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node",
        "config": { "includeFiles": ["dist/**"] }
      }
    ],
    app.use(express.static(__dirname + '/public'));

    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ]
  }